<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <properties resource="jdbc.properties" />

    <context id="xxxx" targetRuntime="MyBatis3Simple" defaultModelType="flat">

        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="${jdbc.driver}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.username}"
                        password="${jdbc.password}" />

        <javaModelGenerator targetPackage="cn.com.zzn.persist.model" targetProject="/home/zhouyc/idea_workspace/atm/model">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="cn.com.zzn.persist.mapper" targetProject="/home/zhouyc/idea_workspace/atm/coredao" />
        <javaClientGenerator type="XMLMAPPER" targetPackage="cn.com.zzn.persist.client" targetProject="/home/zhouyc/idea_workspace/zzn/coredao" />
        <!--<table schema="bobohouse" tableName="atm_external_user" />-->
        <!--<table schema="bobohouse" tableName="atm_pending_mngasn" />-->
        <table schema="bobohouse" tableName="atm_project_info" />
        <!--<table schema="bobohouse" tableName="atm_sign_in" />-->
        <!--<table schema="bobohouse" tableName="atm_external_user" />-->
    </context>
</generatorConfiguration>
